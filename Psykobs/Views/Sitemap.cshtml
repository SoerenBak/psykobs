@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Sitemap>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
	Layout = "master.cshtml";
}


<header class="main-background header-text">
	<div class="container text-center">
		<h1>@Model.Name</h1>
	</div>
</header>
<div class="sitemap">
	<div class="container">
		<div class="col-lg-12">
			@Umbraco.RenderMacro("sitemap")
		</div>
	</div>
</div>
